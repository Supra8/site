<header itemscope itemtype="http://schema.org/WPHeader" class="header">
  <div class="container">
    <h1 class="logo">
      <a href="<%- config.root %>">
        <svg viewBox="1 -3 14 14" height="20" width="20" xmlns="http://www.w3.org/2000/svg">
          <path d="M11.5 8L8.8 5.4 6.6 8.5 5.5 1.6 2.38 8H0V10h3.6L4.5 8.2l0.9 5.4L9 8.5l1.6 1.5H14V8H11.5z"/>
        </svg>
        <span class="enginsight" itemprop="name"><%- config.title %></span>
      </a>
    </h1>
    <nav itemscope itemtype="http://schema.org/SiteNavigationElement" class="nav-items">
      <ul class="items">
        <li class="item">
          <a itemprop="url" href="<%- config.root %>contact/">
            <span itemprop="name"><%- __('navigation.contact') %></span>
          </a>
        </li>
        <!--<li class="item">
          <a itemprop="url" href="//blog.enginsight.com">
            <span itemprop="name">Blog</span>
          </a>
        </li>-->
        <li class="item">
          <a itemprop="url" href="https://app.enginsight.com/">
            <span itemprop="name"><%- __('navigation.login') %></span>
          </a>
        </li>
        <li class="item language">
          <% var language = 'en'; if(config.language === 'en') language = 'de'; %>
          <% if(page.path) { %>
            <a itemprop="url" href="/<%- language %>/<%- page.path %>">
          <% } else { %>
            <a itemprop="url" href="/<%- language %>/<%- page.title !== 'index' ? page.title + '/': '' %>">
          <% } %>
            <span itemprop="name">
              <svg xmlns="http://www.w3.org/2000/svg" fill="#999999" height="16" viewBox="0 0 24 24" width="16">
                <path d="M0 0h24v24H0z" fill="none"></path>
                <path d="M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm-1 17.93c-3.95-.49-7-3.85-7-7.93 0-.62.08-1.21.21-1.79L9 15v1c0 1.1.9 2 2 2v1.93zm6.9-2.54c-.26-.81-1-1.39-1.9-1.39h-1v-3c0-.55-.45-1-1-1H8v-2h2c.55 0 1-.45 1-1V7h2c1.1 0 2-.9 2-2v-.41c2.93 1.19 5 4.06 5 7.41 0 2.08-.8 3.97-2.1 5.39z"></path>
              </svg><%- language %>
            </span>
          </a>
        </li>
      </ul>
    </nav>
    <div id="mobile-nav">
      <svg xmlns="http://www.w3.org/2000/svg" fill="#000000" height="24" viewBox="0 0 24 24" width="24">
        <path d="M0 0h24v24H0z" fill="none"/>
        <path d="M3 18h18v-2H3v2zm0-5h18v-2H3v2zm0-7v2h18V6H3z"/>
      </svg>
    </div>
  </div>
</header>
